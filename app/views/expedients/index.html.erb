<div class="form-group col-sm-8">
  <h4>Expedientes</h4>
</div>

<div class="table-responsive">
<table class="table table-hover table-sm">
  <thead class="thead-light">
    <tr>
      <th scope="col">No Expediente</th>
      <th scope="col">AÃ±o</th>
      <th scope="col">Activo</th>
      <th scope="col">Actor</th>
      <th scope="col">Defendant</th>
      <th scope="col">Customer</th>
      <th scope="col">Fee</th>
      <th scope="col">Payment</th>
      <th scope="col">Saldo</th>
      <th scope="col">Court</th>
      <th scope="col">State</th>
      <th scope="col">District</th>
      <th scope="col">Act</th>
      <th scope="col">Matter</th>
      <th scope="col">Judgment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @expedients.each do |expedient| %>
      <tr scope="row">
        <td><%= expedient.NoExpedient %></td>
        <td><%= expedient.year %></td>
        <td><%= expedient.active %></td>
        <td><%= expedient.actor %></td>
        <td><%= expedient.defendant %></td>
        <td><%= expedient.customer %></td>
        <td><%= expedient.fee %></td>
        <td><%= expedient.payment %></td>
        <td><%= expedient.saldo %></td>
        <td><%= expedient.court %></td>
        <td><%= expedient.state %></td>
        <td><%= expedient.district %></td>
        <td><%= expedient.act %></td>
        <td><%= expedient.matter %></td>
        <td><%= expedient.judgment %></td>
        <td><%= link_to 'Consultar', expedient , class: "btn btn-outline-primary btn-sm"%></td>
        <td><%= link_to 'Modificar', edit_expedient_path(expedient) , class: "btn btn-outline-primary btn-sm"%></td>
        <td><%= link_to 'Eliminar', expedient, method: :delete, data: { confirm: 'Estas Seguro?' }, class: "btn btn-outline-primary btn-sm" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

